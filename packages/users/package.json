{
  "name": "@cyberchess.org/cyberchess-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "pull:admin": "rimraf amplify && cp -r ../admin/amplify ./amplify && amplify configure project",
    "update:admin": "amplify env pull",
    "deploy": "amplify publish",
    "status": "amplify status && git status",
    "push": "amplify push",
    "switch:env": "sh ./scripts/switch-env.sh"
  },
  "dependencies": {
    "autoprefixer": "^9.7.1",
    "aws-amplify": "^1.2.4",
    "aws-amplify-vue": "^0.3.3",
    "core-js": "^3.3.2",
    "fibers": "^4.0.2",
    "register-service-worker": "^1.6.2",
    "rimraf": "^3.0.0",
    "sweetalert": "^2.1.2",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.0",
    "vuex": "^3.0.1",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-pwa": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^1.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0"
  }
}
